{
  "name": "Dark",
  "description": "Modern dark theme optimized for low-light environments with high contrast and reduced eye strain",
  "version": "1.0.0",
  "author": "Repository Reporting System",
  "created": "2025-01-28",
  "supports_dark_mode": true,
  "colors": {
    "primary": "#60a5fa",
    "success": "#34d399",
    "warning": "#fbbf24",
    "error": "#f87171",
    "background": "#111827",
    "surface": "#1f2937",
    "text_primary": "#f9fafb",
    "text_secondary": "#d1d5db"
  },
  "typography": {
    "font_family": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
    "font_size_base": "16px",
    "line_height_base": "1.6"
  },
  "accessibility": {
    "wcag_level": "AA",
    "min_contrast_ratio": 7.0,
    "supports_high_contrast": true,
    "supports_reduced_motion": true,
    "optimized_for_low_light": true
  },
  "features": {
    "responsive": true,
    "print_optimized": true,
    "mobile_first": true,
    "css_variables": true,
    "reduced_blue_light": true
  }
}
